<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="paperformat_loan" model="report.paperformat">
            <field name="name">Loan Employee</field>
            <field name="default" eval="True"/>
            <field name="format">Letter</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">50</field>
            <field name="margin_bottom">30</field>
            <field name="margin_left">20</field>
            <field name="margin_right">20</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">35</field>
            <field name="dpi">90</field>
        </record>

        <report id="action_report_employee_loan"
                string="Loan Employee"
                model="hr.employee.loan"
                report_type="qweb-pdf"
                file="hr_payroll_co_intello.loan_employee"
                name="hr_payroll_co_intello.loan_employee"/>

        <report id="action_report_employee_loan_status"
                string="Account State"
                model="hr.employee.loan"
                report_type="qweb-pdf"
                file="hr_payroll_co_intello.loan_employee_status"
                name="hr_payroll_co_intello.loan_employee_status"/>

    </data>
</odoo>