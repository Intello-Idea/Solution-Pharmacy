<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Masive Sequence -->
        <record id="hr_salary_change_id" model="ir.sequence">
            <field name="name">Cambio de Salario</field>
            <field name="code">hr.change.salary</field>
            <field name="active">TRUE</field>
            <field name="prefix">CC/</field>
            <field name="padding">10</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

         <record id="hr_social_security_id" model="ir.sequence">
            <field name="name">Cambio de Administradora</field>
            <field name="code">hr.change.administrator</field>
            <field name="active">TRUE</field>
            <field name="prefix">CA/</field>
            <field name="padding">10</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- single Sequence -->

        <record id="hr_social_security_single_id" model="ir.sequence">
            <field name="name">Cambio de Administradora Individual</field>
            <field name="code">hr.change.administrator.single</field>
            <field name="active">TRUE</field>
            <field name="prefix">CAI/</field>
            <field name="padding">10</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="hr_salary_change_single_id" model="ir.sequence">
            <field name="name">Cambio de Salario Individual</field>
            <field name="code">hr.change.salary.single</field>
            <field name="active">TRUE</field>
            <field name="prefix">CCI/</field>
            <field name="padding">10</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>


    </data>
</odoo>