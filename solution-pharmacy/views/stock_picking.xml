<odoo>
    <data>
        <record id="solution_pharmacy_add_field_stock" model="ir.ui.view">
            <field name="name">stock.picking_add_field</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/notebook/page/field[@name='move_ids_without_package']/tree/field[@name='product_id']" position="after">
                    <field name="name"/>
                </xpath>
                <xpath expr="//page/field[@name='move_ids_without_package']/.." position="after">
                    <page string="Raw material">
                        <field name="raw_material">
                            <tree editable="bottom">
                                <field name="product_id"/>
                                <field name="product_qty"/>
                                <field name="percentage"/>
                                <field name="material_stock" invisible="1"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>