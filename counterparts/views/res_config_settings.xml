<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_two" model="ir.ui.view">
            <field name="name">res.config.settings.form_two</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="30"/>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@class='settings']/div[@data-key='stock']/div[3]" position="inside">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                            <field name="counterpart_products"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="counterpart_products"/>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>

    </data>
</odoo>